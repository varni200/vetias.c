#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

#include <stdio.h>

int main() {

    int n;

    // Read input

    scanf("%d", &n);

    // Print numbers from 1 to n

    for (int i = 1; i <= n; i++) {

        printf("%d", i);

        if (i < n) {

            printf(" ");

        }

    }

    // Move to the next line after printing all numbers

    printf("\n");

    return 0;

}
