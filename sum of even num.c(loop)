#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

     int n, sum = 0;

    
    scanf("%d", &n);

    // Calculate sum of even numbers from 1 to n
    for (int i = 1; i <= n; i++) {
        if (i % 2 == 0) {
            sum += i;
        }
    }

    // Display the result
    printf("%d\n", sum);
    return 0;
}
